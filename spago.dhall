{ name = "jelly"
, dependencies =
  [ "aff"
  , "arrays"
  , "console"
  , "effect"
  , "either"
  , "foreign"
  , "foreign-object"
  , "free"
  , "js-timers"
  , "maybe"
  , "newtype"
  , "prelude"
  , "record"
  , "refs"
  , "strings"
  , "tailrec"
  , "transformers"
  , "tuples"
  , "web-dom"
  , "web-events"
  , "web-html"
  ]
, packages = ./packages.dhall
, sources = [ "src/**/*.purs", "test/**/*.purs" ]
, license = "MIT"
, repository = "https://github.com/yukikurage/purescript-jelly.git"
, backend = "purs-backend-es build"
}
